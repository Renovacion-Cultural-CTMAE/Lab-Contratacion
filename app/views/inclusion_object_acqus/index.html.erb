<div class="w-full">
  <% if notice.present? %>
    <p class="py-2 px-3 bg-green-50 mb-5 text-green-500 font-medium rounded-lg inline-block" id="notice"><%= notice %></p>
  <% end %>

  <div class="flex justify-between items-center">
    <h1 class="font-bold text-4xl">Certificado de inclusion de objeto en el plan anual de adquisiciones</h1>
    <%= link_to 'Crear un nuevo certificado de inclusion de objeto en el plan anual de adquisiciones', new_inclusion_object_acqu_path, class: "rounded-lg py-3 px-5 bg-blue-600 text-white block font-medium" %>
  </div>

  <div id="inclusion_object_acqus" class="min-w-full">
    <%= render @inclusion_object_acqus %>
  </div>
</div>
