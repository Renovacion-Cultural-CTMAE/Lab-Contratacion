<div id="<%= dom_id phone %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Cedula de contratistas:</strong>
    <%= phone.cedula_de_contratistas %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Numero de telefono:</strong>
    <%= phone.numero_de_telefono %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Operador:</strong>
    <%= phone.operador %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Activo:</strong>
    <%= phone.activo %>
  </p>

  <% if action_name != "show" %>
    <%= link_to "Show this phone", phone, class: "rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium" %>
    <%= link_to 'Edit this phone', edit_phone_path(phone), class: "rounded-lg py-3 ml-2 px-5 bg-gray-100 inline-block font-medium" %>
    <hr class="mt-6">
  <% end %>
</div>
