<div id="<%= dom_id contractor %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Nombre:</strong>
    <%= contractor.nombre %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Apellido:</strong>
    <%= contractor.apellido %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Cedula contratista:</strong>
    <%= contractor.cedula_contratista %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Lugar expedicion cedula:</strong>
    <%= contractor.lugar_expedicion_cedula %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Fecha expedicion cedula:</strong>
    <%= contractor.fecha_expedicion_cedula %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Edad:</strong>
    <%= contractor.edad %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Fecha de nacimiento:</strong>
    <%= contractor.fecha_de_nacimiento %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Municipio de recidencia:</strong>
    <%= contractor.municipio_de_recidencia %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Sexo:</strong>
    <%= contractor.sexo %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Numero de libreta militar:</strong>
    <%= contractor.numero_de_libreta_militar %>
  </p>

  <% if action_name != "show" %>
    <%= link_to "Show this contractor", contractor, class: "rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium" %>
    <%= link_to 'Edit this contractor', edit_contractor_path(contractor), class: "rounded-lg py-3 ml-2 px-5 bg-gray-100 inline-block font-medium" %>
    <hr class="mt-6">
  <% end %>
</div>
