<div id="<%= dom_id contract_supervisor %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Cedula supervisor:</strong>
    <%= contract_supervisor.cedula_supervisor %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Numero contrato:</strong>
    <%= contract_supervisor.numero_contrato %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Fecha inicio supervicion:</strong>
    <%= contract_supervisor.fecha_inicio_supervicion %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Cedula contratista:</strong>
    <%= contract_supervisor.cedula_contratista %>
  </p>

  <% if action_name != "show" %>
    <%= link_to "Show this contract supervisor", contract_supervisor, class: "rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium" %>
    <%= link_to 'Edit this contract supervisor', edit_contract_supervisor_path(contract_supervisor), class: "rounded-lg py-3 ml-2 px-5 bg-gray-100 inline-block font-medium" %>
    <hr class="mt-6">
  <% end %>
</div>
